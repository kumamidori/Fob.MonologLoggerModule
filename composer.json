{
    "name": "fob/app-logger-module",
    "type": "library",
    "description": "Application Logger Module for BEAR.Sunday",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Nana Yamane",
            "email": "shigematsu.nana@gmail.com"
        }
    ],
    "keywords": [
        "BEAR.Sunday","Monolog"
    ],
    "require": {
        "php": ">=7.2.0",
        "ray/aop": "~2.9",
        "ray/di": "~2.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "~8.5"
    },
    "autoload": {
        "psr-4": {
            "Fob\\AppLogger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fob\\AppLogger\\": "tests"
        }
    },
    "scripts": {
        "cs": [
            "php-cs-fixer fix -v --dry-run",
            "phpcs --standard=./phpcs.xml src"
        ],
        "cs-fix": [
            "php-cs-fixer fix -v",
            "phpcbf src"
        ]
    }
}
